GENERATE_SOURCEMAP=false
# Fast refresh for better development experience
FAST_REFRESH=true
# Disable ESLint warnings in overlay (optional)
ESLINT_NO_DEV_ERRORS=true
# Disable browser caching during development
DISABLE_ESLINT_PLUGIN=true
# Additional cache busting
WDS_SOCKET_HOST=localhost
WDS_SOCKET_PORT=0
WDS_SOCKET_PATH=/ws
